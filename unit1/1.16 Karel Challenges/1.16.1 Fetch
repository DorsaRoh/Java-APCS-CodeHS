//fetch karel

public class FetchKarel extends SuperKarel
{
    public void run()
    {
        move();
        turnLeft();
        jump();
        
        turnRight();
        move();
        takeBall();
        
        endPosition();
        
    }
    
    private void jump()
    {
        for(int i=0; i<4;i++)
        {
            move();
        }
    }
    
    private void endPosition()
    {
        turnAround();
        move();
        turnLeft();
        jump();
        turnRight();
        move();
        turnAround();
        
        putBall();
    }
}
